{
    "name": "laratrist/laravel-royalty",
    "description": "A user points package for Laravel that can be used to give rewards, loyalty or experience points with real time support",
    "keywords": [
        "laratrist",
        "laravel",
        "royalty",
        "loyalty-points",
        "user-points",
        "user-experience-points",
        "user-rewards-points"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kamal Sroor",
            "email": "kamal.s.sroor@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "kalnoy/nestedset": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.5",
        "orchestra/testbench": ">=4.0",
        "laravel/legacy-factories": ">=1.0.4"
    },
    "autoload": {
        "psr-4": {
            "laratrist\\Royalty\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "laratrist\\Royalty\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/database/seeds"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "laratrist\\Royalty\\RoyaltyServiceProvider"
            ],
            "aliases": {
                "Royalty": "laratrist\\Royalty\\Facades\\Royalty"
            }
        }
    }
}
